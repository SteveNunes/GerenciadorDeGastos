<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="vBoxMain" prefHeight="680.0" prefWidth="1150.0" spacing="20.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.PrecosProdutosTabController">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <children>
      <VBox spacing="5.0" VBox.vgrow="ALWAYS">
         <children>
            <TableView fx:id="tableView" VBox.vgrow="ALWAYS">
              <columns>
                <TableColumn fx:id="tableColumnProdutos" editable="false" prefWidth="320.0" sortable="false" text="Produto" />
                <TableColumn fx:id="tableColumnMinPrice" editable="false" prefWidth="110.0" sortable="false" text="Menor preço" />
                  <TableColumn fx:id="tableColumnMinPriceDate" editable="false" prefWidth="110.0" sortable="false" text="Data" />
                  <TableColumn fx:id="tableColumnMaxPrice" editable="false" prefWidth="110.0" sortable="false" text="Maior preço" />
                  <TableColumn fx:id="tableColumnMaxPriceDate" editable="false" prefWidth="110.0" sortable="false" text="Data" />
                  <TableColumn fx:id="tableColumnAvgPrice" editable="false" prefWidth="110.0" sortable="false" text="Preço médio" />
                  <TableColumn fx:id="tableColumnPriceList" editable="false" prefWidth="240.0" sortable="false" text="Lista de preços" />
              </columns>
            </TableView>
            <HBox alignment="CENTER" spacing="10.0">
               <children>
                  <Button fx:id="buttonAdd" maxWidth="30.0" minWidth="30.0" mnemonicParsing="false" prefWidth="30.0" />
                  <Button fx:id="buttonDel" maxWidth="30.0" minWidth="30.0" mnemonicParsing="false" prefWidth="30.0" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</VBox>
